{
    "version": "0.2",
    "language": "en",
    "ignorePaths": [
        "node_modules/**",
        "dist/**",
        ".git/**",
        "*.lock",
        "*.log"
    ],
    "dictionaries": [
        "typescript",
        "javascript",
        "html",
        "css",
        "markdown"
    ],
    "words": [
        "react",
        "jsx",
        "tsx",
        "vite",
        "npm",
        "eslint",
        "Chakra"
    ],
    "overrides": [
        {
            "filename": "**/*.md",
            "language": "en"
        },
        {
            "filename": "**/*.ts",
            "language": "en"
        }
    ]
}